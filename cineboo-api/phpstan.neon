includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/
    # Level 9 is the highest level
    level: 8
    scanFiles:
            - _ide_helper.php
            - _ide_helper_models.php
            - .phpstorm.meta.php
    ignoreErrors:
            - '#PHPDoc tag @mixin contains unknown class App\\Models#'
